{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "prebuildCommand": "npx expo prebuild --platform android --no-install"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "prebuildCommand": "npx expo prebuild --platform android --no-install"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "prebuildCommand": "npx expo prebuild --platform android --no-install"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
